{
  "name": "gatsby-starter-reasonml",
  "description": "Gatsby starter to create static sites using type-safe ReasonML.",
  "version": "2.0.0",
  "author": "Ian Wilson <iwilsonq@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "bs-css": "^11.0.0",
    "bs-date-fns": "^0.1.6",
    "bs-fetch": "^0.5.2",
    "bs-platform": "^7.2.2",
    "bs-webapi": "^0.15.9",
    "core-js": "^3.6.4",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "gatsby": "^2.19.10",
    "gatsby-background-image": "^0.9.14",
    "gatsby-bucklescript-output-linker": "0.0.3",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-manifest": "^2.2.40",
    "gatsby-plugin-offline": "^3.0.33",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-source-filesystem": "^2.1.47",
    "gatsby-transformer-remark": "^2.6.49",
    "gatsby-transformer-sharp": "^2.3.14",
    "lodash.debounce": "^4.0.8",
    "normalize-css": "^2.3.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-transition-group": "^4.3.0",
    "reason-react": "^0.8.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "bsb:build": "bsb -make-world -w",
    "bsb:clean": "bsb -clean-world",
    "start": "run-p start:**",
    "start:bsb": "npm run bsb:build",
    "start:gatsby": "gatsby develop -H 0.0.0.0",
    "start:lambda": "NODE_ENV=development netlify-lambda serve src/lambda",
    "clean": "run-p clean:**",
    "clean:bsb": "npm run bsb:clean",
    "clean:gatsby": "gatsby clean",
    "build": "run-p build:**",
    "build:bsb": "npm run bsb:build",
    "build:gatsby": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "serve": "gatsby serve",
    "postinstall": "bsb -make-world",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.5",
    "mailgun-js": "^0.22.0",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iwilsonq/gatsby-starter-reasonml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
